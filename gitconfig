[user]
    name = David Jonas
    email = djonas@noip.com
[core]
    editor = /usr/bin/vim
[alias]
    st = status
    lg = log --graph --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %an, %ar%Creset'
    lga = log --all --graph --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %an, %ar%Creset'
    cm = commit -m
    out = log origin..HEAD
    unstage = reset HEAD --
    exec = "!exec "
    last = log -1 HEAD
    co = checkout
[color]
    ui = auto
    interactive = true
[push]
    default = current

